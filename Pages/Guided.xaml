<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PayRemind.Pages.Guided"
             Title="Guided"
             xmlns:local="clr-namespace:PayRemind.Pages"
              BackgroundColor="Transparent">

    <VerticalStackLayout x:Name="MainGrid" VerticalOptions="Center">
            <Button x:Name="MyButton" VerticalOptions="Center"
                Text="Tap to See Menu Options" 
                HorizontalOptions="Center"
                    Clicked="MyButton_Clicked"
                    />


        <Button x:Name="MyButtonTwo" VerticalOptions="Center"
                Text="Tap to See Menu Options Two" 
                HorizontalOptions="Center"
                    Clicked="MyButton_Clicked"
                    />

    </VerticalStackLayout>

</ContentPage>