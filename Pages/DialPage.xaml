<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PayRemind.Pages.DialPage"
             Title="DialPage">
    <VerticalStackLayout Spacing="25" Padding="30,0" VerticalOptions="Center">
        <Entry x:Name="numberEntry" Placeholder="Enter phone number" Keyboard="Telephone" />
        <Button x:Name="dialButton" Text="Dial" Clicked="OnDialButtonClicked" />
    </VerticalStackLayout>
</ContentPage>